# Environment variables for E2E testing
# Copy this to .env.test and fill in your values

# Supabase Configuration (use dev/test instance, NOT production!)
PUBLIC_SUPABASE_URL=https://your-project.supabase.co
PUBLIC_SUPABASE_ANON_KEY=your-anon-key-here
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key-here

# Test Admin Credentials
# Create this user in Supabase and set role to 'admin'
TEST_ADMIN_EMAIL=admin@test.com
TEST_ADMIN_PASSWORD=secure-test-password-here

# Test User Credentials (optional - for non-admin tests)
TEST_USER_EMAIL=user@test.com
TEST_USER_PASSWORD=secure-test-password-here

# Base URL for tests (default: http://localhost:5173)
BASE_URL=http://localhost:5173
